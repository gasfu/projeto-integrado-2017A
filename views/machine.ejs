<section class="machine-wrapper --buyed --hide">
	<form class="form" buy-submit>
		<ul data-machine class="machine"></ul>
		<button class="confirm"><img class="icon" src="/assets/images/buy.svg">Comprar</button>
	</form>
</section>

<!-- LOGIN -->
<section class="machine-wrapper --login --hide">
	
	<!-- Demo AFD -->
	<ul class="afd" afd>
		<li afd-item class="item">Q0</li>
		<li afd-item class="item">Q1</li>
		<li afd-item class="item">Q2</li>
		<li afd-item class="item">Q3</li>	
	</ul>

	<form class="form" authenticate-submit >
		<ul data-apts class="machine"></ul>
		<input input-passoword class="password" type="password" name="password" placeholder="informe sua senha" maxlength="4" />	
	</form>
</section>

<!-- FEEDBACK -->
<section class="machine-wrapper --feedback --hide">
	<form class="form" close-submit >
		<ul data-apts class="machine">
			<img class="wink" src="/assets/icons/wink.svg">
			<p class="text" >BOA! ITEM COMPRADO :)</p>
		</ul>
		<button type="submit" class="confirm">Fechar</button>
	</form>
</section>